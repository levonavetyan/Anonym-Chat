<div class="container">
    <div class="span12">
      <%= form_for @room do |f| %>
        <div class="text-light text-center">
          <%= f.label :title %>
          <%= f.text_field :title, autofocus: true, class: 'form-control' %>
        </div>

        <%= f.submit "Add!", class: 'btn btn-primary' %>
      <% end %>
   </div>
</div>
